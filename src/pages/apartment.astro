const storgatan: Apartment = {
  id: 101,
  address: 'Storgatan 1',
  city: 'Stockholm',
  rooms: 3,
  size: 75,
  rent: 10000,
  images: [{ id: 1, url: '/storgatan.jpg' }],
  tenants: [{
      id: 1, name: 'Alice', email: 'alice@example.com',
